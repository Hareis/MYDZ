a:hover,.nav_inner ul li a:hover,.nav_inner ul li .nav_dropdown li a:hover,.footer_link li a:hover,.header_wrap_hd ul.fr li a,.tool_wrap_hd li h2,.quick_menu_hd li span,.btn_wrap_hd span.title,.tab_sort li a:hover,.tab_sort li a.sort_current {
    color:#ffcc66;
}
.header_wrap_hd ul.fl a.header_user:hover,.header_wrap_hd ul.fl a.header_user.selected,.header_wrap_hd ul.fl ul li:hover,.nav_wrap,.sidebar_wrap a span,.tool_wrap_bd li a:hover,.tool_wrap_bd li a.tool_wrap_selected,.tab li a.selected,.tab_tool li a:hover,label.checkbox.checked span,.select2-container-multi .select2-choices .select2-search-choice {
    background-color:#ffcc66;
}
.nav_inner ul li a:hover i {
    border-bottom: 4px solid #ffcc66;
}
footer,input[type=text]:focus,input[type=password]:focus,textarea:focus,.btn_wrap_hd span i:after,.tab li a:after,.tab_tool li a:hover,label.checkbox span,.tab {
    border-color: #ffcc66;
}
.tool_wrap,.quick_menu_hd li span,table.data_table {
    border-color:#ffcc66 #bbbbbb #bbbbbb #bbbbbb;
}
button,input[type=submit] {
    color:#fff;
    background-color: #ffcc66;
    border-color: #c59638;
    text-shadow: 0 -1px 0 rgba(0,0,0,.2);
}
button:hover,input[type=submit]:hover {  
    background-color: #c59638;
    text-decoration:none;
}

.select2-container-active .select2-choice,
.select2-container-active .select2-choices {
    border: 1px solid #ffcc66;
}
.select2-drop {
    border: 1px solid #ffcc66;
    border-top:0px;
}
.select2-results .select2-highlighted {
    background: #ffcc66;
}
.select2-container-active .select2-choice .select2-arrow b {
    background-position:-80px 0px;
}
.select2-dropdown-open .select2-choice .select2-arrow b {
    background-position: -80px -4px;
}
.tab_sort li a:hover i,.tab_sort li a.sort_current i {
    background-position:-63px 0px;
}
.tab_sort li a:hover i.down,.tab_sort li a.sort_current i.down {
    background-position:-63px -8px;
}
.select2-container-multi .select2-choices .select2-search-choice-focus {
    background-color: #d4d4d4;
}
ul.PageNav li input[type=button]:hover,ul.PageNav li span.Current,ul.PageNav li a:hover {
    background:#ffcc66;
    border:#ffcc66 1px solid;
}
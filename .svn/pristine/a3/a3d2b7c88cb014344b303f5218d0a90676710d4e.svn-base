using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace MYDZ.Entity.Order
{
    /// <summary>
    /// 订单明细表
    /// </summary>
    [Serializable]
    public class tbOrdersDetail
    {
        /// <summary>
        /// 订单明细编号
        /// </summary>
        public int OrdersDetailId { get; set; }

        /// <summary>
        /// 订单编号
        /// </summary>
        public string OrdersNumber { get; set; }

        /// <summary>
        /// 商品编号
        /// </summary>
        public int ProductId { get; set; }

        /// <summary>
        /// 商品属性编号
        /// </summary>
        public int ProductProId { get; set; }

        /// <summary>
        /// 销售提成编号
        /// </summary>
        public int SalesCommissionId { get; set; }

        /// <summary>
        /// 商家编码
        /// </summary>
        public string ProductEncoding { get; set; }

        /// <summary>
        /// 商品图片
        /// </summary>
        public string ProductImg { get; set; }

        /// <summary>
        /// 商品名称
        /// </summary>
        public string ProductName { get; set; }

        /// <summary>
        /// 商品数量
        /// </summary>
        public int ProductNumber { get; set; }

        /// <summary>
        /// 商品单价
        /// </summary>
        public decimal ProductPrice { get; set; }

        /// <summary>
        /// 商品规格
        /// </summary>
        public string ProductSku { get; set; }

        /// <summary>
        /// 商品总价
        /// </summary>
        public decimal ProductTotal { get; set; }

        /// <summary>
        /// 是否有退款
        /// </summary>
        public bool IsProductReFund { get; set; }

        /// <summary>
        /// 是否已取消
        /// </summary>
        public bool IsCanceled { get; set; }

        /// <summary>
        /// 退款状态编号(预留)
        /// </summary>
        public int ReFundStatusId { get; set; }

        /// <summary>
        /// 退款状态名称(预留)
        /// </summary>
        public string ReFundStatusName { get; set; }

        /// <summary>
        /// 退款编号
        /// </summary>
        public string ReFundNumber { get; set; }

        /// <summary>
        /// 商品套餐名称
        /// </summary>
        public string PackageName { get; set; }

        /// <summary>
        /// 备注
        /// </summary>
        public string Details { get; set; }

        /// <summary>
        /// 录入日期
        /// </summary>
        public DateTime InputDate { get; set; }

        /// <summary>
        /// 子订单编号
        /// </summary>
        public string SubOrderNumber { get; set; }

        /// <summary>
        /// 商品外部编号
        /// </summary>
        public string OutNumberIId { get; set; }

        /// <summary>
        /// 商品成本价
        /// </summary>
        public decimal ProductCost { get; set; }

        /// <summary>
        /// 优惠金额
        /// </summary>
        public decimal OrdersDiscount { get; set; }

        /// <summary>
        /// 手动调整金额
        /// </summary>
        public decimal OrdersAdjust { get; set; }

        /// <summary>
        /// 应付金额
        /// </summary>
        public decimal OrdersAccounts { get; set; }
    }
}

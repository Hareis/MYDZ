.btn_wrap_bd
{
    float: left;
    height: auto;
    border: 1px solid red;
    margin-top: 10px;
}
.checkbox
{
    min-width: 82px;
    margin-top: 10px;
}
.ListGoodsCats .checkbox em
{
    margin-left: 5px;
}
input[type=text], input[type=password], textarea
{
    width: 80px;
    height: 20px;
    padding: 5px 10px;
    font-size: 12px;
    color: #222;
    background-color: #fff;
    border: 1px solid #ccc;
    -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
    -moz-box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, .075);
    -webkit-transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
    transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
    outline: none;
    resize: none;
}
.ListGoodsCats ol
{
    width: 700px;
    height: 100%;
    margin-left: 140px;
}
.btn_wrap_bd li input[type=text]
{
    width: 89px;
}
.salebd
{
    max-height: 570px;
}
.skucss
{
    width: 80px;
}
.goodsimgsku
{
    float: left;
    margin-left: 10px;
    width: 90px;
    height: 90px;
    border: 1px solid red;
}
.goodsimgsku img
{
    width: 90px;
    height: 90px;
}
.floatdiv
{
    height: 16px;
    width: 90px;
    background-color: #F2F2F2;
    opacity: 0.8;
    line-height: 16px;
    border: 1px solid #ccc;
    position: absolute;
    display: none;
}
.floatdiv a
{
    margin-left: 10px;
}
.freight-tempter, .customer-price
{
    display: none;
}
.freight_payer-div
{
    display: none;
}

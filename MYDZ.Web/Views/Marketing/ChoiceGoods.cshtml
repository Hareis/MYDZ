@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ReturnGift</title>
    <link href="@Url.Content("~/css/ChoiceGoods.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/js/Page.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/js/Marketing/ChoiceGoods.js")" type="text/javascript"></script>
</head>
<body>
    <div style="margin-left: 15px;">
        <div class="selectproduct">
            <div>
                <div class="table_item search-div">
                    <span class="table_hd"><span class="require"></span>条件查找：</span>
                    <div class="table_bd">
                        <input type="text" name="input-text-outerid" value="" placeholder="商家编码" />
                    </div>
                    <div class="table_bd">
                        <input type="text" name="input-text-title" value="" placeholder="商品标题或关键字" />
                    </div>
                    <div class="table_bd" style="margin-left: 20px; margin-top: 1px;">
                        <input type="button" name="searchbutton" value="搜索" />
                    </div>
                </div>
            </div>
            <div id="data_table-div-goods">
            </div>
            <div class="page_box" name="paging">
            </div>
        </div>
    </div>
    <input type="hidden" id="ChoiceIds" name="text-value" />
    <input type=hidden Id="IscheckboxB" value="@ViewData["Ischeckbox"]" />
    <input type=hidden Id="IscaozuoB" value="@ViewData["Iscaozuo"]" />
</body>
</html>

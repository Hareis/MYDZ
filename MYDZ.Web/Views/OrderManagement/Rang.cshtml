@model List<MYDZ.Entity.Order.LogisticsPartner>
<tr>
    <td style="width:100px;text-align:right;padding-top:10px;">收件地址：</td>
    <td style="padding-top:10px;">@ViewBag.Addr</td>
</tr>
<tr>
    <td colspan="2">
        <div class="rang_wrap">
            <table cellpadding="0" cellspacing="0" border="0">
                <thead>
                    <tr>
                        <th width="120">物流名称</th>
                        <th>备注</th>
                    </tr>
                </thead>
                <tbody>
                @foreach (var item in Model) { 
                    <tr>
                        <td>@item.Partner.CompanyName</td>
                        <td style="text-align:left;">@item.UncoverRemark</td>
                    </tr>   
                }
                </tbody>
            </table>
        </div>
    </td>
</tr>